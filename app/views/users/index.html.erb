<%= render 'userinfo', user: @user, book: @book %>


<h1>Users</h1>
<table>
  <tr>
    <th>Image</th>
    <th>Name</th>
    <th></th>
  </tr>
    <% @users.each do |user| %>
  <tr>
    <td><%= user.profile_image, fallback: "no_image.jpg" %></td>
    <td><%= user.name %></td>
    <td><%= link_to "Show", user_path(@user) %></td>
  </tr>
  <% end %>
</table>
